    "color-js": "^1.0.5",
    "dotenv-webpack": "^4.0.0",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^2.9.4",
    "next": "^10.0.0",
    "next-fonts": "^1.4.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-inlinesvg": "^2.1.1",
    "react-sizes": "^2.0.0",
    "styled-jsx": "^3.3.1",
    "styled-jsx-plugin-postcss": "^3.0.2",
    "swr": "^0.3.6",
    "zustand": "^3.1.3",
    "zustand-persist": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
